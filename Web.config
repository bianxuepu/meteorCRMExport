<configuration>
    <system.web>
		<httpRuntime maxRequestLength="102400" executionTimeout="3600" minFreeThreads="8" minLocalRequestFreeThreads="4" appRequestQueueLimit="100" enableVersionHeader="true"/>
		<customErrors mode="Off"></customErrors>
      <compilation debug="true" targetFramework="4.0" />
    </system.web>

</configuration>
